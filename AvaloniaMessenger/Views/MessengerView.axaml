<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaMessenger.Views.MessengerView"
             xmlns:vm="clr-namespace:AvaloniaMessenger.ViewModels"
             xmlns:s="clr-namespace:AvaloniaMessenger.Assets"
             x:DataType="vm:MessengerViewModel">
  <Grid ColumnDefinitions="*,1.5*" RowDefinitions="*,10*">
    <StackPanel Name="ChatPanel" Grid.Row="1" Background="AntiqueWhite">
      
    </StackPanel>
    
    <StackPanel Name="MessagePanel" Grid.Row="1" Grid.Column="1" Background="AliceBlue">

    </StackPanel>

    <DockPanel Name="ToolPanel" Background="PowderBlue">
      
    </DockPanel>
    <DockPanel Name="ChatHeaderPanel" Grid.Column="1" Background="PaleGoldenrod">
    
    </DockPanel>
  </Grid>
</UserControl>
